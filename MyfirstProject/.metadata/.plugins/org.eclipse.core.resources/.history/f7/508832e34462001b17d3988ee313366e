
public class Practice1 {


	public static void main(String[] args) {
		add1();
		add2();
		add3();
		
	}

	//1
	public static void add1() {
	int a=20;
	int b=30;
	int c=a+b;	
	System.out.println("add1 method executed :-"+c);
	}
	//2
	public static void add2() {
	
		int x=20;
		int y=30;
		int z=x+y;
		System.out.println("add2 method executed :-"+z);
	}
	//3
	public static void add3() {
		int m=20;
		int n=30;
		int o=m+n;
		System.out.println("add1 method executed :-"+o);
	}
	
}
