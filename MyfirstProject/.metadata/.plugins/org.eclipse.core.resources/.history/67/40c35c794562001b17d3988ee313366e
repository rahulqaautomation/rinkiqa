
public class Practice1 {


	public static void main(String[] args) {
		add1();
		add2();
		add3();
		
	}

	//1
	public static int add1() {
	int a=20;
	int b=30;
	int c=a+b;	
	return c;
	}
	//2
	public static int add2() {
		int x=20;
		int y=30;
		int z=x+y;
		return z;
	}
	//3
	public static int add3() {
		int m=20;
		int n=30;
		int o=m+n;
		return o;
	}
	
}
